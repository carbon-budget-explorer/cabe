{
	"name": "cabe",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"download:borders": "mkdir -p data && curl https://raw.githubusercontent.com/nvkelso/natural-earth-vector/master/geojson/ne_110m_admin_0_countries.geojson -o data/ne_110m_admin_0_countries.geojson",
		"install:pydeps": "node src/install_python_deps.cjs",
		"postinstall": "npm run install:pydeps"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "^0.5.3",
		"@types/d3": "^7.4.0",
		"@types/svelte-leafletjs": "^0.8.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-v8": "^0.32.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"leaflet": "^1.9.4",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-leafletjs": "^0.10.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-plugin-iso-import": "^1.0.0",
		"vitest": "^0.32.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"d3": "^7.8.5",
		"d3-geo": "^3.1.0",
		"d3-scale": "^4.0.2",
		"d3-scale-chromatic": "^3.0.0",
		"layercake": "^7.6.1",
		"pyodide": "^0.24.0-alpha.1",
		"svelte-vega": "^2.0.0"
	},
	"overrides": {
		"svelte": "^4.0.5"
	}
}
